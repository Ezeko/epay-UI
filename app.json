{
  "expo": {
    "name": "ePay",
    "slug": "ePay",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "privacy": "public",
    "packagerOpts": {
      "sourceExts": [
        "js",
        "jsx",
        "svg",
        "ts",
        "tsx"
      ]
    },
    "android": {
      "package": "com.app.ePay",
      "versionCode": 1
    },
    "version": "1.0.0",
